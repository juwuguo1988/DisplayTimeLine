<resources>

  <!-- Base application theme. -->
  <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
    <!-- Customize your theme here. -->
    <item name="colorPrimary">@color/colorPrimary</item>
    <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
    <item name="colorAccent">@color/colorAccent</item>
  </style>

  <style name="s_333333_14">
    <item name="android:textSize">14sp</item>
    <item name="android:textColor">@color/c_333333</item>
  </style>

  <style name="s_666666_13">
    <item name="android:textSize">14sp</item>
    <item name="android:textColor">@color/c_666666</item>
  </style>


  <!-- 透明绿字绿边按钮 -->
  <style name="common_blue_side_text_button">
    <item name="android:layout_width">fill_parent</item>
    <item name="android:layout_height">44dip</item>
    <item name="android:background">@drawable/btn_transf_blue_side_bg</item>
    <item name="android:textColor">@color/c_2bb2ba</item>
    <item name="android:textSize">16sp</item>
    <item name="android:gravity">center</item>
  </style>

</resources>
